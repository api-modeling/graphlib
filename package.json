    "lint:types": "tsc",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint",
    "test": "web-test-runner test/**/*.test.js test/*.test.js --coverage --node-resolve --playwright --browsers chromium firefox webkit",
